{
  "name": "Minimalist Ad blocker",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["declarativeNetRequest"],
  "description": "Blocks ads on websites",
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
